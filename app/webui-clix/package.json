{
  "name": "clix-terminal",
  "version": "1.0.0",
  "description": "Revolutionary web-based terminal interface for LocalAgent",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "serve": "npm run build && npm run preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-websocket": "^4.8.1",
    "xterm": "^5.3.0",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-canvas": "^0.5.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-search": "^0.13.0",
    "xterm-addon-unicode11": "^0.6.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0",
    "zustand": "^4.4.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-dropzone": "^14.2.3",
    "localforage": "^1.10.0",
    "idb": "^8.0.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vite-bundle-analyzer": "^0.7.0",
    "@playwright/test": "^1.40.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.8",
    "jsdom": "^23.0.1",
    "workbox-cli": "^7.0.0"
  },
  "keywords": [
    "terminal",
    "web-terminal",
    "xterm",
    "react",
    "typescript",
    "pwa",
    "websocket",
    "localagent",
    "collaboration",
    "webgl"
  ],
  "author": "LocalAgent Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/localagent/clix-terminal.git"
  },
  "bugs": {
    "url": "https://github.com/localagent/clix-terminal/issues"
  },
  "homepage": "https://github.com/localagent/clix-terminal#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "Chrome >= 100",
    "Firefox >= 100",
    "Safari >= 16",
    "Edge >= 100"
  ]
}